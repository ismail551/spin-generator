{
  "name": "backoffice-ic-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate": "npm run gn-instruction && npm run gn-referentiel",
    "gn-instruction": "ng-openapi-gen -c src/swaggers/config/instruction-api-config.json",
    "gn-referentiel": "ng-swagger-gen -c src/swaggers/config/referentiel-api-config.json",
    "format": "prettier --write \"src/**/*.+(ts|html|json)\"",
    "format-check": "prettier --check \"src/**/*.+(ts|html|json)\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.0.4",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/material": "^17.0.4",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@ng-select/ng-select": "^12.0.7",
    "@popperjs/core": "^2.11.8",
    "@swimlane/ngx-datatable": "^20.1.0",
    "autoprefixer": "^10.4.16",
    "ngx-skeleton-loader": "^8.1.0",
    "ngx-toastr": "^18.0.0",
    "postcss": "^8.4.32",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.0.0",
    "tailwindcss": "^3.3.6",
    "tslib": "^2.3.0",
    "uuid": "^9.0.1",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.7",
    "@angular/cli": "^17.0.7",
    "@angular/compiler-cli": "^17.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/uuid": "^9.0.8",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-openapi-gen": "^0.51.0",
    "ng-swagger-gen": "^2.3.1",
    "prettier": "^3.1.1",
    "typescript": "~5.2.2"
  }
}
